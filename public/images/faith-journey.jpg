<!-- Faith Journey Image - SVG -->
<svg width="800" height="450" viewBox="0 0 800 450" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="skyGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E0F6FF;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="sunGradient" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#FFD700;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FFA500;stop-opacity:1" />
    </radialGradient>
  </defs>
  
  <!-- Sky background -->
  <rect width="800" height="300" fill="url(#skyGradient)" />
  
  <!-- Ground -->
  <rect y="300" width="800" height="150" fill="#8FBC8F" />
  
  <!-- Sun -->
  <circle cx="650" cy="80" r="40" fill="url(#sunGradient)" />
  
  <!-- Clouds -->
  <ellipse cx="150" cy="80" rx="30" ry="20" fill="white" opacity="0.8" />
  <ellipse cx="170" cy="75" rx="25" ry="18" fill="white" opacity="0.8" />
  <ellipse cx="130" cy="75" rx="20" ry="15" fill="white" opacity="0.8" />
  
  <!-- Church building -->
  <rect x="300" y="200" width="200" height="150" fill="#8B4513" />
  <polygon points="300,200 400,120 500,200" fill="#654321" />
  
  <!-- Church cross -->
  <rect x="395" y="100" width="10" height="30" fill="#FFD700" />
  <rect x="385" y="110" width="30" height="8" fill="#FFD700" />
  
  <!-- Church door -->
  <rect x="380" y="280" width="40" height="70" fill="#4A4A4A" />
  <path d="M 380 280 Q 400 270 420 280 L 420 350 L 380 350 Z" fill="#654321" />
  
  <!-- Windows -->
  <rect x="320" y="230" width="30" height="40" fill="#4169E1" />
  <rect x="450" y="230" width="30" height="40" fill="#4169E1" />
  <line x1="335" y1="230" x2="335" y2="270" stroke="white" stroke-width="2" />
  <line x1="320" y1="250" x2="350" y2="250" stroke="white" stroke-width="2" />
  <line x1="465" y1="230" x2="465" y2="270" stroke="white" stroke-width="2" />
  <line x1="450" y1="250" x2="480" y2="250" stroke="white" stroke-width="2" />
  
  <!-- Family figures -->
  <!-- Father -->
  <circle cx="120" cy="290" r="15" fill="#D4A574" />
  <rect x="110" y="305" width="20" height="30" fill="#4A5568" />
  <rect x="112" y="335" width="16" height="15" fill="#2D3748" />
  
  <!-- Mother -->
  <circle cx="160" cy="290" r="15" fill="#F4C2A1" />
  <rect x="150" y="305" width="20" height="30" fill="#E53E3E" />
  <rect x="152" y="335" width="16" height="15" fill="#2D3748" />
  
  <!-- Children -->
  <circle cx="200" cy="300" r="10" fill="#D4A574" />
  <rect x="195" y="310" width="10" height="20" fill="#4299E1" />
  <circle cx="230" cy="305" r="8" fill="#F4C2A1" />
  <rect x="226" y="313" width="8" height="17" fill="#F687B3" />
  
  <!-- Path to church -->
  <path d="M 250 350 Q 320 340 380 350" stroke="#DDD" stroke-width="8" fill="none" />
  
  <!-- Trees -->
  <circle cx="100" cy="250" r="25" fill="#228B22" />
  <rect x="95" y="275" width="10" height="25" fill="#8B4513" />
  
  <circle cx="680" cy="280" r="30" fill="#228B22" />
  <rect x="675" y="310" width="10" height="40" fill="#8B4513" />
  
  <!-- Birds -->
  <path d="M 250 100 Q 260 95 270 100 Q 260 105 250 100" fill="#333" />
  <path d="M 520 120 Q 530 115 540 120 Q 530 125 520 120" fill="#333" />
  
  <!-- Text overlay -->
  <rect x="50" y="380" width="700" height="60" fill="rgba(0,0,0,0.7)" rx="10" />
  <text x="400" y="405" font-family="Arial, sans-serif" font-size="20" fill="white" text-anchor="middle" font-weight="bold">Mon Parcours de Foi</text>
  <text x="400" y="425" font-family="Arial, sans-serif" font-size="14" fill="#E2E8F0" text-anchor="middle">Né, grandi et baptisé dans l'église - Une famille unie dans la foi</text>
</svg>