<!-- Theological Formation Image - SVG -->
<svg width="800" height="450" viewBox="0 0 800 450" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1E40AF;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#3B82F6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#60A5FA;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="bookGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8B4513;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#A0522D;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="450" fill="url(#bgGradient)" />
  
  <!-- Study desk -->
  <rect x="100" y="250" width="600" height="20" fill="#8B4513" rx="10" />
  <rect x="120" y="270" width="15" height="80" fill="#654321" />
  <rect x="665" y="270" width="15" height="80" fill="#654321" />
  
  <!-- Open Bible -->
  <rect x="300" y="200" width="200" height="50" fill="url(#bookGradient)" rx="5" />
  <rect x="305" y="205" width="190" height="40" fill="#FFF8DC" rx="3" />
  <line x1="400" y1="205" x2="400" y2="245" stroke="#8B4513" stroke-width="2" />
  
  <!-- Bible text lines -->
  <line x1="320" y1="215" x2="390" y2="215" stroke="#333" stroke-width="1" />
  <line x1="320" y1="220" x2="385" y2="220" stroke="#333" stroke-width="1" />
  <line x1="320" y1="225" x2="390" y2="225" stroke="#333" stroke-width="1" />
  <line x1="320" y1="230" x2="380" y2="230" stroke="#333" stroke-width="1" />
  <line x1="320" y1="235" x2="390" y2="235" stroke="#333" stroke-width="1" />
  
  <line x1="410" y1="215" x2="480" y2="215" stroke="#333" stroke-width="1" />
  <line x1="410" y1="220" x2="475" y2="220" stroke="#333" stroke-width="1" />
  <line x1="410" y1="225" x2="480" y2="225" stroke="#333" stroke-width="1" />
  <line x1="410" y1="230" x2="470" y2="230" stroke="#333" stroke-width="1" />
  <line x1="410" y1="235" x2="480" y2="235" stroke="#333" stroke-width="1" />
  
  <!-- Study books stack -->
  <rect x="150" y="230" width="100" height="15" fill="#DC143C" rx="3" />
  <rect x="155" y="215" width="90" height="15" fill="#228B22" rx="3" />
  <rect x="160" y="200" width="80" height="15" fill="#4169E1" rx="3" />
  
  <!-- Notebook -->
  <rect x="550" y="220" width="80" height="30" fill="#FFF" rx="3" stroke="#CCC" stroke-width="1" />
  <line x1="560" y1="230" x2="620" y2="230" stroke="#333" stroke-width="1" />
  <line x1="560" y1="235" x2="615" y2="235" stroke="#333" stroke-width="1" />
  <line x1="560" y1="240" x2="620" y2="240" stroke="#333" stroke-width="1" />
  
  <!-- Pen -->
  <rect x="540" y="225" width="3" height="20" fill="#FF4500" />
  <rect x="540" y="220" width="3" height="5" fill="#FFD700" />
  
  <!-- Computer/laptop -->
  <rect x="200" y="180" width="80" height="50" fill="#2F2F2F" rx="5" />
  <rect x="205" y="185" width="70" height="35" fill="#000" rx="3" />
  <rect x="235" y="230" width="20" height="10" fill="#D3D3D3" rx="2" />
  
  <!-- Graduation cap -->
  <polygon points="650,150 680,140 710,150 680,160" fill="#000" />
  <rect x="675" y="145" width="10" height="5" fill="#000" />
  <circle cx="685" cy="140" r="25" fill="#000" />
  <rect x="660" y="138" width="50" height="4" fill="#000" />
  
  <!-- Diploma -->
  <rect x="520" y="160" width="60" height="40" fill="#FFF8DC" rx="3" />
  <rect x="522" y="162" width="56" height="36" fill="#FFF" rx="2" stroke="#D4AF37" stroke-width="2" />
  <circle cx="550" cy="180" r="8" fill="#D4AF37" />
  <text x="550" y="185" font-family="serif" font-size="8" fill="#8B4513" text-anchor="middle">MASTER</text>
  
  <!-- Cross symbol -->
  <rect x="380" y="120" width="8" height="20" fill="#FFD700" />
  <rect x="372" y="128" width="24" height="6" fill="#FFD700" />
  
  <!-- Church icon -->
  <rect x="100" y="120" width="40" height="30" fill="#8B4513" />
  <polygon points="100,120 120,100 140,120" fill="#654321" />
  <rect x="117" y="95" width="6" height="15" fill="#FFD700" />
  <rect x="110" y="102" width="20" height="4" fill="#FFD700" />
  
  <!-- Piano keys -->
  <rect x="600" y="300" width="150" height="30" fill="#FFF" stroke="#000" stroke-width="1" />
  <rect x="612" y="300" width="8" height="18" fill="#000" />
  <rect x="630" y="300" width="8" height="18" fill="#000" />
  <rect x="660" y="300" width="8" height="18" fill="#000" />
  <rect x="678" y="300" width="8" height="18" fill="#000" />
  <rect x="696" y="300" width="8" height="18" fill="#000" />
  <rect x="714" y="300" width="8" height="18" fill="#000" />
  <rect x="732" y="300" width="8" height="18" fill="#000" />
  
  <!-- Musical notes -->
  <circle cx="680" cy="280" r="3" fill="#000" />
  <rect x="683" y="270" width="1" height="10" fill="#000" />
  <circle cx="700" cy="275" r="3" fill="#000" />
  <rect x="703" y="265" width="1" height="10" fill="#000" />
  
  <!-- École Pneuma logo placeholder -->
  <rect x="50" y="350" width="60" height="40" fill="#4A5568" rx="5" />
  <text x="80" y="375" font-family="Arial, sans-serif" font-size="10" fill="white" text-anchor="middle" font-weight="bold">ÉCOLE</text>
  <text x="80" y="385" font-family="Arial, sans-serif" font-size="10" fill="white" text-anchor="middle" font-weight="bold">PNEUMA</text>
  
  <!-- Light rays from cross -->
  <g opacity="0.3">
    <line x1="384" y1="130" x2="300" y2="80" stroke="#FFD700" stroke-width="2" />
    <line x1="384" y1="130" x2="450" y2="70" stroke="#FFD700" stroke-width="2" />
    <line x1="384" y1="130" x2="320" y2="180" stroke="#FFD700" stroke-width="2" />
    <line x1="384" y1="130" x2="480" y2="170" stroke="#FFD700" stroke-width="2" />
  </g>
  
  <!-- Text overlay -->
  <rect x="50" y="380" width="700" height="60" fill="rgba(0,0,0,0.8)" rx="10" />
  <text x="400" y="405" font-family="Arial, sans-serif" font-size="20" fill="white" text-anchor="middle" font-weight="bold">Formation et Responsabilités</text>
  <text x="400" y="425" font-family="Arial, sans-serif" font-size="14" fill="#E2E8F0" text-anchor="middle">Master en Développement Web • Formation École Pneuma • Service Musical</text>
</svg>